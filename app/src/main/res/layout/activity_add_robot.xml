<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/AddRobot_rootLayout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <vp.metagram.ui.RobotUI.NonSwipeableViewPager
        android:id="@+id/AddRobot_viewPager"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/AddRobot_indicator"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </vp.metagram.ui.RobotUI.NonSwipeableViewPager>


    <me.relex.circleindicator.CircleIndicator
        android:id="@+id/AddRobot_indicator"
        android:layout_width="match_parent"
        android:layout_height="49dp"
        android:layoutDirection="ltr"
        android:background="@color/sCoolGrayC3"
        app:ci_drawable="@drawable/shape_indicator_black"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>